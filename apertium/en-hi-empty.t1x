    <def-macro n="elimina_GDND" npar="1">
      <choose>
        <when>
          <test>
            <equal>
              <clip part="gen" pos="1" side="tl"/>
              <lit-tag v="GD"/>
            </equal>
          </test>
          <let>
            <clip part="gen" pos="1" side="tl"/>
            <lit-tag v="m"/>
          </let>
        </when>
      </choose>
      <choose>
        <when>
          <test>
            <equal>
              <clip part="nbr" pos="1" side="tl"/>
              <lit-tag v="ND"/>
            </equal>
          </test>
          <let>
            <clip part="nbr" pos="1" side="tl"/>
            <lit-tag v="sg"/>
          </let>
        </when>
      </choose>
    </def-macro>
    <def-macro n="posa_GDND" npar="1">
      <!--per a posar valors de GD i ND en adjectius i determinants, que venen del bilingüe sense cap valor i per tant és impossible fer-hi cap operació ni canvi -->
      <choose>
        <when>
          <!--si la paraula no té valor per a gènere (i en aquest cas, tampoc per a nombre) -->
          <test>
            <equal>
              <clip part="gen" pos="1" side="tl"/>
              <lit v=""/>
            </equal>
          </test>
          <let>
            <var n="paraula1"/>
            <clip part="lemh" pos="1" side="tl"/>
          </let>
          <append n="paraula1">
            <clip part="tags" pos="1" side="tl"/>
            <lit-tag v="GD"/>
            <lit-tag v="ND"/>
            <clip part="lemq" pos="1" side="tl"/>
          </append>
          <let>
            <clip part="whole" pos="1" side="tl"/>
            <var n="paraula1"/>
          </let>
        </when>
        <when>
          <!--si la paraula no té valor per a nombre (però sí per a gènere, ja que sinó hauria entrat pel when anterior) -->
          <test>
            <equal>
              <clip part="nbr" pos="1" side="tl"/>
              <lit v=""/>
            </equal>
          </test>
          <let>
            <var n="paraula1"/>
            <clip part="lemh" pos="1" side="tl"/>
          </let>
          <append n="paraula1">
            <clip part="tags" pos="1" side="tl"/>
            <lit-tag v="ND"/>
            <clip part="lemq" pos="1" side="tl"/>
          </append>
          <let>
            <clip part="whole" pos="1" side="tl"/>
            <var n="paraula1"/>
          </let>
        </when>
      </choose>
    </def-macro>
