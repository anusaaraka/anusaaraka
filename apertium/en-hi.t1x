              <clip part="a_verb" pos="2" side="tl"/>
              <lit-tag v="3"/>
              <lit-tag v="4"/>
              <lit-tag v="5"/>
              <clip part="lemq" pos="2" side="tl"/>
            </concat>
          </let>
        </when>
        <when>
          <test>
            <and>
              <equal>
