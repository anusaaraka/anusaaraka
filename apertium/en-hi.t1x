          <let>
            <var n="nbr_chunk"/>
            <clip part="nbr" pos="1" side="tl"/>
          </let>
          <let>
            <var n="verbfinal"/>
            <concat>
              <clip part="lemh" pos="1" side="tl"/>
              <clip part="a_verb" pos="1" side="tl"/>
              <lit-tag v="3"/>
              <lit-tag v="4"/>
              <lit-tag v="5"/>
              <clip part="lemq" pos="1" side="tl"/>
            </concat>
          </let>
        </when>
        <when>
          <test>
            <or>
              <equal>
                <clip part="temps" pos="1" side="sl"/>
                <lit-tag v="inf"/>
              </equal>
              <equal>
                <clip part="temps" pos="1" side="sl"/>
                <lit-tag v="ger"/>
              </equal>
            </or>
          </test>
          <let>
            <var n="temps_chunk"/>
