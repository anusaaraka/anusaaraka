    <def-macro n="f_concord2" npar="2">
      <!--1 = main parameter (typically noun); 2 = secondary parameter (typically adj or det) -->
      <let>
        <var n="gen_chunk"/>
        <lit v=""/>
      </let>
      <let>
        <var n="nbr_chunk"/>
        <lit v=""/>
      </let>
      <choose>
        <!--posa GD i ND a la paraula si no té valor per a gènere o per a nombre (excepte números) -->
        <when>
          <test>
            <not>
              <equal>
                <clip part="a_num" pos="2" side="tl"/>
                <lit-tag v="num"/>
              </equal>
            </not>
          </test>
          <choose>
            <!--si no te valor per a gènere i , per tant, tampoc per a nombre: -->
            <when>
              <test>
                <equal>
                  <clip part="gen" pos="2" side="tl"/>
                  <lit v=""/>
                </equal>
              </test>
              <let>
                <var n="paraula1"/>
                <clip part="lemh" pos="2" side="tl"/>
              </let>
              <append n="paraula1">
                <clip part="tags" pos="2" side="tl"/>
                <lit-tag v="GD"/>
                <lit-tag v="ND"/>
                <clip part="lemq" pos="2" side="tl"/>
              </append>
              <let>
                <clip part="whole" pos="2" side="tl"/>
                <var n="paraula1"/>
              </let>
            </when>
            <when>
              <!--si la paraula no té valor per a nombre (però sí per a gènere, ja que sinó hauria entrat pel when anterior) -->
              <test>
                <equal>
                  <clip part="nbr" pos="2" side="tl"/>
                  <lit v=""/>
                </equal>
              </test>
              <let>
                <var n="paraula1"/>
                <clip part="lemh" pos="2" side="tl"/>
              </let>
              <append n="paraula1">
                <clip part="tags" pos="2" side="tl"/>
                <lit-tag v="ND"/>
                <clip part="lemq" pos="2" side="tl"/>
              </append>
              <let>
                <clip part="whole" pos="2" side="tl"/>
                <var n="paraula1"/>
              </let>
            </when>
          </choose>
        </when>
      </choose>
      <!--OPERACIONS DE GÈNERE -->
      <choose>
        <when>
          <test>
            <equal>
              <clip part="gen" pos="2" side="tl"/>
              <lit-tag v="GD"/>
            </equal>
          </test>
          <choose>
            <when>
              <test>
                <equal>
                  <clip part="gen" pos="1" side="tl"/>
                  <lit-tag v="GD"/>
                </equal>
              </test>
              <let>
                <var n="gen_chunk"/>
                <lit-tag v="GD"/>
              </let>
            </when>
            <when>
              <test>
                <not>
                  <!--si gènere de pos 1 és masc o fem -->
                  <or>
                    <equal>
                      <clip part="gen" pos="1" side="tl"/>
                      <lit-tag v="mf"/>
                    </equal>
                    <equal>
                      <clip part="gen" pos="1" side="tl"/>
                      <lit v=""/>
                    </equal>
                  </or>
                </not>
              </test>
              <let>
                <clip part="gen" pos="2" side="tl"/>
                <clip part="gen" pos="1" side="tl"/>
              </let>
              <let>
                <var n="gen_chunk"/>
                <clip part="gen" pos="1" side="tl"/>
              </let>
            </when>
            <otherwise>
              <!--si gènere pos 1 és mf -->
              <let>
                <var n="gen_chunk"/>
                <lit-tag v="GD"/>
              </let>
            </otherwise>
          </choose>
        </when>
      </choose>
      <choose>
        <when>
          <test>
            <equal>
              <clip part="gen" pos="1" side="tl"/>
              <lit-tag v="GD"/>
            </equal>
          </test>
          <let>
            <var n="gen_chunk"/>
            <lit-tag v="GD"/>
          </let>
        </when>
        <when>
          <test>
            <not>
              <or>
                <equal>
                  <clip part="gen" pos="1" side="tl"/>
                  <lit-tag v="mf"/>
                </equal>
                <equal>
                  <clip part="gen" pos="1" side="tl"/>
                  <lit v=""/>
                </equal>
              </or>
            </not>
          </test>
          <let>
            <var n="gen_chunk"/>
            <clip part="gen" pos="1" side="tl"/>
          </let>
        </when>
        <otherwise>
          <!--si gènere de pos 1 és mf -->
          <choose>
            <when>
              <test>
                <!--si el gènere de pos 2 és GD, el gènere del chunk serà tb GD (deixant pos 1 com a mf) per si es pot assignar gènere a pos 2 (p. ex. 'the pacient - el/la pacient') -->
                <not>
                  <equal>
                    <clip part="gen" pos="2" side="tl"/>
                    <lit-tag v="GD"/>
                  </equal>
                </not>
              </test>
              <let>
                <var n="gen_chunk"/>
                <clip part="gen" pos="1" side="tl"/>
              </let>
            </when>
          </choose>
        </otherwise>
      </choose>
      <!--OPERACIONS DE NOMBRE -->
      <choose>
        <when>
          <test>
            <equal>
              <clip part="nbr" pos="2" side="tl"/>
              <lit-tag v="ND"/>
            </equal>
          </test>
          <choose>
            <when>
              <test>
                <equal>
                  <clip part="nbr" pos="1" side="tl"/>
                  <lit-tag v="ND"/>
                </equal>
              </test>
              <let>
                <var n="nbr_chunk"/>
                <lit-tag v="ND"/>
              </let>
            </when>
            <when>
              <test>
                <not>
                  <!--si nombre de pos 1 és masc o fem -->
                  <or>
                    <equal>
                      <clip part="nbr" pos="1" side="tl"/>
                      <lit-tag v="sp"/>
                    </equal>
                    <equal>
                      <clip part="nbr" pos="1" side="tl"/>
                      <lit v=""/>
                    </equal>
                  </or>
                </not>
              </test>
              <let>
                <clip part="nbr" pos="2" side="tl"/>
                <clip part="nbr" pos="1" side="tl"/>
              </let>
              <let>
                <var n="nbr_chunk"/>
                <clip part="nbr" pos="1" side="tl"/>
              </let>
            </when>
            <otherwise>
              <!--si nombre pos 1 és sp -->
              <let>
                <var n="nbr_chunk"/>
                <lit-tag v="ND"/>
              </let>
            </otherwise>
          </choose>
        </when>
      </choose>
      <choose>
        <when>
          <test>
            <equal>
              <clip part="nbr" pos="1" side="tl"/>
              <lit-tag v="ND"/>
            </equal>
          </test>
          <let>
            <var n="nbr_chunk"/>
            <lit-tag v="ND"/>
          </let>
        </when>
        <when>
          <test>
            <not>
              <or>
                <equal>
                  <clip part="nbr" pos="1" side="tl"/>
                  <lit-tag v="sp"/>
                </equal>
                <equal>
                  <clip part="nbr" pos="1" side="tl"/>
                  <lit v=""/>
                </equal>
              </or>
            </not>
          </test>
          <let>
            <var n="nbr_chunk"/>
            <clip part="nbr" pos="1" side="tl"/>
          </let>
        </when>
        <otherwise>
          <!--si nombre de pos 1 és sp -->
          <choose>
            <when>
              <test>
                <!--si el nombre de pos 2 és ND, el nombre del chunk serà tb ND (deixant pos 1 com a sp) per si es pot assignar nombre a pos 2  -->
                <not>
                  <equal>
                    <clip part="nbr" pos="2" side="tl"/>
                    <lit-tag v="ND"/>
                  </equal>
                </not>
              </test>
              <let>
                <var n="nbr_chunk"/>
                <clip part="nbr" pos="1" side="tl"/>
              </let>
            </when>
          </choose>
        </otherwise>
      </choose>
    </def-macro>
