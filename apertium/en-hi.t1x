    <def-macro n="passa_inf_a_pres" npar="1">
      <choose>
        <when>
          <test>
            <equal>
              <!--En aquest cas, si el tagger s'equivoca i tria 'inf', ho tractarem com si fos 'pres'-->
              <clip part="temps" pos="1" side="sl"/>
              <lit-tag v="inf"/>
            </equal>
          </test>
          <let>
            <var n="temps_chunk"/>
            <lit-tag v="pri"/>
          </let>
          <let>
            <var n="pers_chunk"/>
            <lit-tag v="PD"/>
          </let>
          <let>
            <var n="nbr_chunk"/>
            <lit-tag v="ND"/>
          </let>
          <let>
            <var n="lema_SV"/>
            <lit v="verbcj"/>
          </let>
          <let>
            <var n="verbfinal"/>
            <concat>
              <clip part="lemh" pos="1" side="tl"/>
              <clip part="a_verb" pos="1" side="tl"/>
              <lit-tag v="3"/>
              <lit-tag v="4"/>
              <lit-tag v="5"/>
              <clip part="lemq" pos="1" side="tl"/>
            </concat>
          </let>
        </when>
      </choose>
    </def-macro>
    <def-macro n="passa_pp_a_ifip" npar="1">
      <choose>
        <when>
          <!--si entra com a 'pp', ho tractarem com si fos 'past', per tant quedarà en forma perifràstica (ifip)-->
          <test>
            <equal>
              <clip part="temps" pos="1" side="sl"/>
              <lit-tag v="pp"/>
            </equal>
          </test>
          <let>
            <var n="temps_chunk"/>
            <lit-tag v="ifip"/>
          </let>
          <let>
            <var n="pers_chunk"/>
            <lit-tag v="PD"/>
          </let>
          <let>
            <var n="nbr_chunk"/>
            <lit-tag v="ND"/>
          </let>
          <let>
            <var n="lema_SV"/>
            <lit v="verbcj"/>
          </let>
          <let>
            <var n="verbfinal"/>
            <concat>
              <lit v="anar"/>
              <lit-tag v="vaux"/>
              <lit-tag v="4"/>
              <lit-tag v="5"/>
              <lit v="$ ^"/>
              <clip part="lemh" pos="1" side="tl"/>
              <clip part="a_verb" pos="1" side="tl"/>
              <lit-tag v="inf"/>
              <clip part="lemq" pos="1" side="tl"/>
            </concat>
          </let>
        </when>
      </choose>
    </def-macro>
