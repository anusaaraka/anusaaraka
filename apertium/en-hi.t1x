                <clip part="temps" pos="1" side="sl"/>
                <lit-tag v="past"/>
              </equal>
              <equal>
                <clip part="nbr" pos="1" side="sl"/>
                <lit-tag v="sg"/>
              </equal>
            </and>
          </test>
          <let>
            <var n="temps_chunk"/>
            <lit-tag v="pii"/>
          </let>
          <let>
            <var n="pers_chunk"/>
            <lit-tag v="PD"/>
          </let>
          <let>
            <var n="nbr_chunk"/>
            <clip part="nbr" pos="1" side="tl"/>
          </let>
          <let>
            <var n="verbfinal"/>
            <concat>
              <clip part="lemh" pos="2" side="tl"/>
              <clip part="a_verb" pos="2" side="tl"/>
              <lit-tag v="3"/>
              <lit-tag v="4"/>
              <lit-tag v="5"/>
              <clip part="lemq" pos="2" side="tl"/>
            </concat>
          </let>
        </when>
        <when>
          <test>
            <equal>
              <clip part="temps" pos="1" side="sl"/>
              <lit-tag v="pres"/>
            </equal>
          </test>
          <let>
            <var n="temps_chunk"/>
            <lit-tag v="pri"/>
          </let>
          <let>
            <var n="pers_chunk"/>
            <lit-tag v="PD"/>
          </let>
          <let>
            <var n="nbr_chunk"/>
            <lit-tag v="ND"/>
          </let>
          <let>
            <clip part="lemh" pos="1" side="tl"/>
            <lit v="estar"/>
          </let>
          <let>
            <clip part="a_verb" pos="1" side="tl"/>
            <lit-tag v="vblex"/>
          </let>
          <let>
            <var n="verbfinal"/>
            <concat>
              <clip part="lemh" pos="1" side="tl"/>
              <clip part="a_verb" pos="1" side="tl"/>
              <lit-tag v="3"/>
              <lit-tag v="4"/>
              <lit-tag v="5"/>
              <clip part="lemq" pos="1" side="tl"/>
            </concat>
          </let>
        </when>
        <when>
          <test>
            <equal>
              <clip part="temps" pos="1" side="sl"/>
              <lit-tag v="pri"/>
            </equal>
          </test>
          <let>
            <clip part="lemh" pos="1" side="tl"/>
            <lit v="estar"/>
          </let>
          <let>
            <clip part="a_verb" pos="1" side="tl"/>
            <lit-tag v="vblex"/>
